{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "ghactions",
      "camelcase": "GHActions",
      "title": "Github Actions",
      "scope": "source.ghactions",
      "file-types": [
        "ghactions"
      ],
      "injection-regex": "^ghactions$",
      "class-name": "TreeSitterGHActions"
    }
  ],
  "metadata": {
    "version": "0.2.2",
    "license": "MIT",
    "description": "Parser for Github Actions expressions",
    "authors": [
      {
        "name": "Robert Muir",
        "email": "rmuir@apache.org",
        "url": "https://github.com/rmuir"
      }
    ],
    "links": {
      "repository": "https://github.com/rmuir/tree-sitter-ghactions"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}
