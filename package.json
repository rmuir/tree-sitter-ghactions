{
  "name": "tree-sitter-ghactions",
  "version": "0.2.1",
  "description": "Parser for Github Actions expressions",
  "repository": "https://github.com/rmuir/tree-sitter-ghactions",
  "license": "MIT",
  "author": {
    "name": "Robert Muir",
    "email": "rmuir@apache.org",
    "url": "https://github.com/rmuir"
  },
  "main": "bindings/node",
  "types": "bindings/node",
  "keywords": [
    "incremental",
    "parsing",
    "tree-sitter",
    "github",
    "actions"
  ],
  "files": [
    "grammar.js",
    "tree-sitter.json",
    "binding.gyp",
    "prebuilds/**",
    "bindings/node/*",
    "queries/*",
    "src/**",
    "*.wasm"
  ],
  "dependencies": {
    "node-addon-api": "^8.3.1",
    "node-gyp-build": "^4.8.4"
  },
  "devDependencies": {
    "prebuildify": "^6.0.1",
    "tree-sitter-cli": "^0.25.9",
    "tree-sitter": "^0.25.0",
    "eslint": "^9.36.0",
    "eslint-config-treesitter": "^1.0.2",
    "typescript": "^5.9.2"
  },
  "peerDependencies": {
    "tree-sitter": "^0.22.4"
  },
  "peerDependenciesMeta": {
    "tree-sitter": {
      "optional": true
    }
  },
  "scripts": {
    "install": "node-gyp-build",
    "prestart": "tree-sitter build --wasm",
    "start": "tree-sitter playground",
    "lint": "eslint --max-warnings 0 grammar.js",
    "typecheck": "tsc --noEmit",
    "test": "node --test bindings/node/*_test.js"
  }
}
